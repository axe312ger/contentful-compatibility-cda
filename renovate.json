{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "baseBranches": [
    "main",
    "cma"
  ],
  "packageRules": [
    {
      "matchFileNames": [
        "environments/*/package.json"
      ],
      "groupName": null
    }
  ],
  "separateMinorPatch": true,
  "dependencyDashboard": true,
  "prHourlyLimit": 1,
  "prConcurrentLimit": 42
}
